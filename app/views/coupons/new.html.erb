<h1>Coupon Form</h1>

<%= form_tag coupons_path do %>
  <label>Store:</label><br>
  <%= text_field_tag :"store" %>

  <label>Coupon Code:</label><br>
  <%= text_field_tag :"coupon_code" %>

<% end %>


fill_in 'coupon_code', with: "YAYFREE"
